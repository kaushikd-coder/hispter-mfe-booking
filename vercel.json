{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "headers": [
        {
            "source": "/remoteEntry.js",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=60, s-maxage=60, must-revalidate"
                },
                {
                    "key": "Cross-Origin-Resource-Policy",
                    "value": "cross-origin"
                }
            ]
        }
    ]
}